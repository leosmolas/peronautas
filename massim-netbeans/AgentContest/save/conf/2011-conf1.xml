<?xml version="1.0" encoding="UTF-8"?>
<!--DOCTYPE conf SYSTEM "helpers/2010/config.dtd"-->
<!-- launch-sync-type = timer or key-->
<conf   backuppath="backup"
		launch-sync-type="key"
		reportpath="./backup/"
		time-to-launch="10000"
		tournamentmode="0"
		tournamentname="Mars2011">
	<simulation-server><network-agent backlog="10" port="12300"/></simulation-server>
	<match>
		<simulation
				configurationclass="massim.competition2011.GraphSimulationConfiguration"
				file-simulationlog="./backup/"
				id="mydemo"
				rmixmlobsserverhost="localhost"
				rmixmlobsserverport="1099"
				rmixmlobserver="massim.competition2011.GraphSimulationRMIXMLDocumentObserver"
				simulationclass="massim.competition2011.GraphSimulation"
				
		>
			<configuration
					xmlns:meta="http://www.tu-clausthal.de/"
					maxNumberOfSteps="100"
					numberOfAgents="20"
					numberOfTeams="2"
					agentsPerTeam="10"
					numberOfNodes="20"
					gridWidth="5"
					gridHeight="5"
					cellWidth="100"
					minNodeWeight="1"
					maxNodeWeight="10"
					minEdgeCost="1"
					maxEdgeCost="10"
			>
				<actions>
					<action
						name="attack"
						energyCost="2" energyCostFailed="2" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="parry"
						energyCost="2" energyCostFailed="2" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="goto"
						energyCost="1" energyCostFailed="1" energyCostDisabled="1" energyCostFailedDisabled="1"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="probe"
						energyCost="1" energyCostFailed="1" energyCostDisabled="1" energyCostFailedDisabled="1"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="survey"
						energyCost="1" energyCostFailed="1" energyCostDisabled="1" energyCostFailedDisabled="1"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="inspect"
						energyCost="2" energyCostFailed="2" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="repair"
						energyCost="2" energyCostFailed="2" energyCostDisabled="3" energyCostFailedDisabled="3"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="buy"
						energyCost="2"  energyCostFailed="2" energyCostDisabled="2"  energyCostFailedDisabled="2"
						healthCost="0"  healthCostFailed="0" healthCostDisabled="0"  healthCostFailedDisabled="0"
						pointsCost="10" pointsCostFailed="0" pointsCostDisabled="20" pointsCostFailedDisabled="20"
					/>
					<action
						name="recharge"
						energyCost="20" energyCostFailed="0"  energyCostDisabled="10" energyCostFailedDisabled="0"
						healthCost="0"  healthCostFailed="0"  healthCostDisabled="0"  healthCostFailedDisabled="0"
						pointsCost="0"  pointsCostFailed="0"  pointsCostDisabled="0"  pointsCostFailedDisabled="0"
					/>
				</actions>
				<roles>
					<role name="Explorer"
							maxEnergy="12" maxBuyEnergy="1000"  rateBuyEnergy="1" maxEnergyDisabled="12" rateBuyEnergyDisabled="1"
							maxHealth="4" maxBuyHealth="1000"  rateBuyHealth="1"
							strength="0" maxBuyStrength="0"  rateBuyStrength="1"
							visRange="2"  maxBuyVisRange="1000" rateBuyVisRange="1">
						<actions>
							<action name="goto"/>
							<action name="probe"/>
							<action name="survey"/>
							<action name="buy"/>
							<action name="recharge"/>
						</actions>
						<actionsDisable>
							<action name="recharge"/>
							<action name="goto"/>
						</actionsDisable>
					</role>
					<role name="Repairer"
							maxEnergy="8" maxBuyEnergy="1000"  rateBuyEnergy="1" maxEnergyDisabled="8" rateBuyEnergyDisabled="1"
							maxHealth="6" maxBuyHealth="1000"  rateBuyHealth="1"
							strength="0" maxBuyStrength="0"  rateBuyStrength="1"
							visRange="1"  maxBuyVisRange="1000" rateBuyVisRange="1">
						<actions>
							<action name="goto"/>
							<action name="parry"/>
							<action name="survey"/>
							<action name="buy"/>
							<action name="repair"/>
							<action name="recharge"/>
						</actions>
						<actionsDisable>
							<action name="recharge"/>
							<action name="goto"/>
							<action name="repair"/>
						</actionsDisable>
					</role>
					<role name="Saboteur"
							maxEnergy="7" maxBuyEnergy="1000"  rateBuyEnergy="1" maxEnergyDisabled="7" rateBuyEnergyDisabled="1"
							maxHealth="3" maxBuyHealth="1000"  rateBuyHealth="1"
							strength="3" maxBuyStrength="1000"  rateBuyStrength="1"
							visRange="1"  maxBuyVisRange="1000" rateBuyVisRange="1">
						<actions>
							<action name="goto"/>
							<action name="parry"/>
							<action name="survey"/>
							<action name="buy"/>
							<action name="attack"/>
							<action name="recharge"/>
						</actions>
						<actionsDisable>
							<action name="recharge"/>
							<action name="goto"/>
						</actionsDisable>
					</role>
					<role name="Sentinel"
							maxEnergy="10" maxBuyEnergy="1000"  rateBuyEnergy="1" maxEnergyDisabled="10" rateBuyEnergyDisabled="1"
							maxHealth="1" maxBuyHealth="1000"  rateBuyHealth="1"
							strength="0" maxBuyStrength="0"  rateBuyStrength="1"
							visRange="3"  maxBuyVisRange="1000" rateBuyVisRange="1">
						<actions>
							<action name="goto"/>
							<action name="parry"/>
							<action name="survey"/>
							<action name="buy"/>
							<action name="recharge"/>
						</actions>
						<actionsDisable>
							<action name="recharge"/>
